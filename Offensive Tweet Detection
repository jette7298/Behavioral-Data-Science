 type it contains, which will be used as input features for the classification model.\n"]},{"cell_type":"code","execution_count":16,"id":"a0901245","metadata":{"execution":{"iopub.execute_input":"2025-11-12T14:49:04.352293Z","iopub.status.busy":"2025-11-12T14:49:04.350725Z","iopub.status.idle":"2025-11-12T14:49:04.364866Z","shell.execute_reply":"2025-11-12T14:49:04.363245Z"},"papermill":{"duration":0.041337,"end_time":"2025-11-12T14:49:04.36709","exception":false,"start_time":"2025-11-12T14:49:04.325753","status":"completed"},"tags":[]},"outputs":[],"source":["# function for counting self-generated words, on unigram level\n","get_generated_words <- function(\n","    positive_words = get(\"positive_words\", envir = parent.frame()),\n","    adverb_words = get(\"adverb_words\", envir = parent.frame()),\n","    negative_words = get(\"negative_words\", envir = parent.frame()),\n","    negation_words = get(\"negation_words\", envir = parent.frame()),\n","    unigram_new = get(\"unigram_new\", envir = parent.frame()))\n","{\n","    # create an empty list to collect all the feature tables\n","      generated_word_features <- list()\n","\n","\n","    # mark all words in the positive list\n","    positive_words <- positive_words %>%\n","      rename(unigram = word) %>%          # make column name match\n","      mutate(number = 1)\n","  \n","    positive.count <- unigram_new %>%\n","    left_join(positive_words, by = \"unigram\", relationship =\"many-to-many\") %>%   # join by unigram word\n","    replace_na(list(number = 0)) %>%                # if not in list -> number = 0\n","    group_by(id) %>%\n","    summarise(positive.count = sum(number * n), .groups = \"drop\") %>% \n","    arrange(id)\n","  \n","    generated_word_features <- append(generated_word_features, list(positive.count))\n","\n","\n","    #Negation words\n","    negation_words <- negation_words %>%\n","      rename(unigram = word) %>%\n","      mutate(number = 1)\n","  \n","    negation.count <- unigram_new %>%\n","    left_join(negation_words, by = \"unigram\", relationship =\"many-to-many\") %>%\n","    replace_na(list(number = 0)) %>%\n","    group_by(id) %>%\n","    summarise(negation.count = sum(number * n), .groups = \"drop\") %>%\n","    arrange(id) \n","  \n","    generated_word_features <- append(generated_word_features, list(negation.count))\n","\n","\n","    #Negtive words\n","    negative_words <- negative_words %>%\n","      rename(unigram = word) %>%\n","      mutate(number = 1)\n","  \n","    negative.count <- unigram_new %>%\n","    left_join(negative_words, by = \"unigram\", relationship =\"many-to-many\") %>%\n","    replace_na(list(number = 0)) %>%\n","    group_by(id) %>%\n","    summarise(negative.count = sum(number * n), .groups = \"drop\") %>%\n","    arrange(id) \n","  \n","    generated_word_features <- append(generated_word_features, list(negative.count))\n","\n","    #Adverb count\n","    adverb_words <- adverb_words %>%\n","      rename(unigram = word) %>%\n","      mutate(number = 1)\n","  \n","    adverb.count <- unigram_new %>%\n","    left_join(adverb_words, by = \"unigram\", relationship =\"many-to-many\") %>%\n","    replace_na(list(number = 0)) %>%\n","    group_by(id) %>%\n","    summarise(adverb.count = sum(number * n), .groups = \"drop\") %>%\n","    arrange(id)\n","  \n","    generated_word_features <- append(generated_word_features, list(adverb.count))\n","\n","\n","    #combine everything\n","    generated_word_output <- reduce(\n","    generated_word_features,\n","    function(x, y) left_join(x, y, by = \"id\")\n","  )\n","\n","    #Also include the labels\n","    generated_word_output <- generated_word_output %>%\n","  left_join(traindat %>% distinct(id, label), by = \"id\", relationship =\"many-to-many\") %>%\n","  relocate(label, .after = id)\n","  \n","  return(generated_word_output)\n","}"]},{"cell_type":"code","execution_count":17,"id":"77833d04","metadata":{"execution":{"iopub.execute_input":"2025-11-12T14:49:04.416987Z"