bigram) %>%\n","      mutate(number = 1)\n","\n","    adv.negative.bigram.count <- bigram_raw %>%\n","      left_join(adverb_negative_bigram, by = c(\"bigram\" = \"bigram\"), relationship = \"many-to-many\") %>%\n","      group_by(id) %>%\n","      replace_na(list(number = 0)) %>%\n","      summarise(adv.negative.bigram.count = sum(number * n), .groups = \"drop\") %>%\n","      arrange(id)\n","\n","    bigram_generated_words <- append(bigram_generated_words, list(adv.negative.bigram.count))\n","\n","\n","    # negation + positive (e.g., \"not smart\", \"never good\")\n","    negation_positive_bigram <- negation_words %>%\n","      cross_join(positive_words, suffix = c(\".adverb\", \".keyword\")) %>%   # keep your suffix style\n","      mutate(bigram = paste(word.adverb, word.keyword)) %>%\n","      select(bigram) %>%\n","      mutate(number = 1)\n","\n","    negat.positive.bigram.count <- bigram_raw %>%\n","      left_join(negation_positive_bigram, by = c(\"bigram\" = \"bigram\"), relationship = \"many-to-many\") %>%\n","      group_by(id) %>%\n","      replace_na(list(number = 0)) %>%\n","      summarise(negated.pos.bigram.count = sum(number * n), .groups = \"drop\") %>%\n","      arrange(id)\n","\n","    bigram_generated_words <- append(bigram_generated_words, list(negat.positive.bigram.count))\n","\n","      #negation + negative (e.g., \"not bad\", \"never terrible\")\n","    negation_negative_bigram <- negation_words %>%\n","      cross_join(negative_words, suffix = c(\".adverb\", \".keyword\")) %>%\n","      mutate(bigram = paste(word.adverb, word.keyword)) %>%\n","      select(bigram) %>%\n","      mutate(number = 1)\n","\n","    negat.negative.bigram.count <- bigram_raw %>%\n","      left_join(negation_negative_bigram, by = c(\"bigram\" = \"bigram\"), relationship = \"many-to-many\") %>%\n","      group_by(id) %>%\n","      replace_na(list(number = 0)) %>%\n","      summarise(negated.neg.bigram.count = sum(number * n), .groups = \"drop\") %>%\n","      arrange(id)\n","\n","    bigram_generated_words <- append(bigram_generated_words, list(negat.negative.bigram.count))\n","\n","\n","      # Combine all per-id features into one df\n","    bigram_generated_word_output <- reduce(\n","      bigram_generated_words,\n","      function(x, y) left_join(x, y, by = \"id\")\n","    )\n","\n","      #we will also add the labels\n","      bigram_generated_word_output <- bigram_generated_word_output %>%\n","  left_join(traindat %>% distinct(id, label), by = \"id\") %>%\n","  relocate(label, .after = id)\n","\n","    return(bigram_generated_word_output)\n","      }, error = function(e){\n","    message(\"bigram_raw probably missing or malformed: \", e$message)\n","    return(NULL)\n","  })\n","}"]},{"cell_type":"code","execution_count":19,"id":"8b59eba5","metadata":{"execution":{"iopub.execute_input":"2025-11-12T14:49:06.789798Z","iopub.status.busy":"2025-11-12T14:49:06.78832Z","iopub.status.idle":"2025-11-12T14:49:12.557636Z","shell.execute_reply":"2025-11-12T14:49:12.555226Z"},"papermill":{"duration":5.797737,"end_time":"2025-11-12T14:49:12.560745","exception":false,"start_time":"2025-11-12T14:49:06.763008","status":"completed"},"tags":[]},"outputs":[{"data":{"text/html":["<table class=\"dataframe\">\n","<caption>A tibble: 6 × 6</caption>\n","<thead>\n","\t<tr><th scope=col>id</th><th scope=col>label</th><th scope=col>adv.positive.bigram.count</th><th scope=col>adv.negative.bigram.count</th><th scope=col>negated.pos.bigram.count</th><th scope=col>negated.neg.bigram.count</th></tr>\n","\t<tr><th scope=col>&lt;chr&gt;</th><th scope=col>&lt;int&gt;</th><th scope=col>&lt;dbl&gt;</th><th scope=col>&lt;dbl&gt;</th><th scope=col>&lt;dbl&gt;</th><th scope=col>&lt;dbl&gt;</th></tr>\n","</thead>\n","<tbody>\n","\t<tr><td>1    </td><td>0</td><td>0</td><td>0</td><td>0</td><td>0</td></tr>\n","\t<tr><td>10   </td><td>0</td><td>0</td><td>0</td><td>0</td><td>0</td></tr>\n","\t<tr><td>100  </td><td>1</td><td>0</td><td>0</td><td>0</td><td>0</td></tr>\n